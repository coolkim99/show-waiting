ingCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"269","messages":"270","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"271","usedDeprecatedRules":"130"},{"filePath":"272","messages":"273","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"130"},{"filePath":"274","messages":"275","errorCount":0,"fatalErrorCount":0,"warningCount":8,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},"/Users/cool/show-waiting/src/index.js",[],["276","277"],"/Users/cool/show-waiting/src/App.js",[],"/Users/cool/show-waiting/src/reportWebVitals.js",[],"/Users/cool/show-waiting/src/Routers.js",["278","279"],"import React from \"react\";\nimport { BrowserRouter, Route, withRouter } from \"react-router-dom\";\nimport PublicRoute from \"./lib/router/PublicRoute\";\nimport PrivateRoute from \"./lib/router/PrivateRoute\";\n\nimport HomePage from \"./pages/HomePage\";\nimport LoginPage from \"./pages/LoginPage\";\nimport JoinPage from \"./pages/JoinPage\";\nimport MainPage from \"./pages/MainPage\";\nimport MenuPage from \"./pages/MenuPage\";\nimport OrderListPage from \"./pages/OrderListPage\";\nimport ManagePage from \"./pages/ManagePage\";\nimport ConfirmPage from \"./pages/ConfirmPage\";\nimport MyOrdersPage from \"./pages/MyOrdersPage\";\n\nconst Routers = () => {\n    return (\n        <BrowserRouter>\n            <Route path=\"/\" exact={true} component={HomePage}/>\n            <Route path=\"/login\" component={LoginPage}/>\n            <Route path=\"/join\" component={JoinPage}/>\n            <Route path=\"/main\" component={MainPage}/>\n            <Route path=\"/menuList/:storeId/:name\" component={MenuPage}/>\n            <Route path=\"/orders\" component={OrderListPage}/>\n            <Route path=\"/manage/:storeId\" component={ManagePage} />\n            <Route path=\"/confirm\" component={ConfirmPage}/>\n            <Route path=\"/myorders\" component={MyOrdersPage}/>\n        </BrowserRouter>\n    )\n}\n\nexport default withRouter(Routers);","/Users/cool/show-waiting/src/pages/HomePage.js",[],"/Users/cool/show-waiting/src/pages/LoginPage.js",[],"/Users/cool/show-waiting/src/pages/JoinPage.js",[],"/Users/cool/show-waiting/src/lib/router/PublicRoute.js",[],"/Users/cool/show-waiting/src/lib/router/PrivateRoute.js",[],"/Users/cool/show-waiting/src/lib/router/isLogin.js",[],"/Users/cool/show-waiting/src/components/auth/AuthTemplate.js",[],"/Users/cool/show-waiting/src/components/home/Home.js",[],"/Users/cool/show-waiting/src/containers/auth/LoginForm.js",["280","281","282"],"import React, { useEffect, useState } from 'react';\nimport { useSelector, useDispatch } from 'react-redux';\nimport { withRouter } from 'react-router-dom';\nimport AuthForm from '../../components/auth/AuthForm';\nimport { changeField, initializeForm, login } from '../../modules/auth';\nimport { check } from '../../modules/user';\nimport { findStores } from '../../modules/stores';\nimport { findDone, findOrdering } from '../../modules/orders';\n\nconst LoginForm = ({ history }) => {\n    const [error, setError] = useState(null);\n    const dispatch = useDispatch();\n    const { form, auth, authError, user } = useSelector(({ auth, user }) => ({\n      form: auth.login,\n      auth: auth.auth,\n      authError: auth.authError,\n      user: user.user\n    }));\n\n    // 인풋 변경 이벤트 핸들러\n    const onChange = e => {\n        const { value, name } = e.target;\n        dispatch(\n          changeField({\n            form: 'login',\n            key: name,\n            value,\n          }),\n        );\n      };\n\n      // 폼 등록 이벤트 핸들러\n    const onSubmit = e => {\n        e.preventDefault();\n        const {email, password} = form;\n        dispatch(login({ email, password }));\n    };\n\n      // 컴포넌트가 처음 렌더링 될 때 form 을 초기화함\n    useEffect(() => {\n        dispatch(initializeForm('login'));\n    }, [dispatch]);\n\n    useEffect(() => {\n        if (authError) {\n          console.log('오류 발생');\n          console.log(authError);\n          setError('로그인 실패');\n          return;\n        }\n        if (auth) {\n          console.log('로그인 성공');\n          console.log(auth.result.id);\n        }\n      }, [auth, authError, dispatch, user]);\n\n      const handleOrders = async (dispatch, userId, history) => {\n        try {\n          await dispatch(findOrdering(userId));\n          await dispatch(findDone(userId));\n          history.push('/orders');\n        } catch (error) {\n          console.error(error);\n        }\n      };\n\n      const fetchData = async () => {\n        try {\n          if (auth) {\n            if (auth.result.type === \"CONSUMER\") {\n              history.push('/main');\n            } else if (auth.result.type === \"STORE\") {\n              await handleOrders(dispatch, auth.result.id, history);\n            }\n          }\n        } catch (error) {\n          console.error(error);\n        }\n      };\n\n      useEffect(() => {        \n        fetchData();\n      }, [history, auth, dispatch]);\n\n\n    return (\n      <AuthForm\n        type=\"login\"\n        form={form}\n        onChange={onChange}\n        onSubmit={onSubmit}\n        error={error}\n      />\n    );\n  };\n  \n  export default withRouter(LoginForm);","/Users/cool/show-waiting/src/components/auth/AuthForm.js",["283","284"],"/Users/cool/show-waiting/src/lib/style/palette.js",[],"/Users/cool/show-waiting/src/components/common/Button.js",[],"/Users/cool/show-waiting/src/modules/auth.js",[],"/Users/cool/show-waiting/src/lib/createRequestSaga.js",[],"/Users/cool/show-waiting/src/lib/api/auth.js",[],"/Users/cool/show-waiting/src/modules/loading.js",[],"/Users/cool/show-waiting/src/lib/api/client.js",[],"/Users/cool/show-waiting/src/modules/index.js",[],"/Users/cool/show-waiting/src/components/common/Responsive.js",[],"/Users/cool/show-waiting/src/containers/auth/JoinForm.js",["285","286","287"],"